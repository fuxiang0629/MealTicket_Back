@using System.Collections.ObjectModel
@using System.Threading
@using System.Web.Http.Description
@using WebAPI.HelpPage.ModelDescriptions

@{ Collection<ParameterDescription> parameters = Model.ResponseParameters; }
@if (parameters.Count > 0)
{
    <table class="help-page-table">
        <thead>
            <tr><th>名称</th><th>描述信息</th><th>级别</th></tr>
        </thead>
        <tbody>
            @{
    foreach (ParameterDescription parameter in parameters)
    {
        var parameterDocumentation = parameter.Documentation ?? "暂无说明.";
            <tr>
                <td class="parameter-name">@parameter.Name</td>
                <td class="parameter-documentation">
                    <p>@parameterDocumentation</p>
                </td>
                <td class="parameter-documentation">
                    <p>@parameter.Level</p>
                </td>
            </tr>
    }
            }
        </tbody>
    </table>
}
else
{
    <p>未知.</p>
}